{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "(lldb) Launch active CMake target",
      "type": "lldb",
      "request": "launch",
      "program": "${command:cmake.launchTargetPath}",
      "cwd": "${command:cmake.launchTargetDirectory}",
      "args": [],
      "stopOnEntry": false,
      "preLaunchTask": "CMake: build",
      "console": "integratedTerminal"
    }
  ]
}
